{
  "Comment": "m365-4-athenabillingmetrics Data acquisition and processing State Machine groups",
  "StartAt": "m365-Collect-athenabillingmetrics",
  "States": {
    "m365-Collect-athenabillingmetrics": {
      "Type": "Task",
      "Resource": "arn:aws:states:::lambda:invoke",
      "Parameters": {
        "FunctionName": "arn:aws:lambda:ap-northeast-1:XXXXXXXXXX:function:m365AthenaBillingMetricsVpc:$LATEST",
        "Payload.$": "$"
      },
      "Catch": [
        {
          "ErrorEquals": [
            "States.ALL"
          ],
          "ResultPath": "$.CollectError",
          "Next": "m365-Collect-athenabillingmetrics-SystemError"
        }
      ],
      "Retry": [
        {
          "ErrorEquals": [
            "States.ALL"
          ],
          "BackoffRate": 1,
          "JitterStrategy": "FULL",
          "IntervalSeconds": 5,
          "MaxAttempts": 10
        }
      ],
      "TimeoutSeconds": 900,
      "ResultPath": "$.CollectResult",
      "Next": "m365-Collect-athenabillingmetrics-CheckStatus"
    },
    "m365-Collect-athenabillingmetrics-CheckStatus": {
      "Type": "Choice",
      "Choices": [
        {
          "And": [
            {
              "Variable": "$.CollectResult.Payload",
              "IsPresent": true
            },
            {
              "Variable": "$.CollectResult.Payload",
              "IsString": true
            },
            {
              "Variable": "$.CollectResult.Payload",
              "StringMatches": "*\"status\"*\"failed\"*"
            }
          ],
          "Next": "m365-Collect-athenabillingmetrics-BusinessFailed"
        }
      ],
      "Default": "m365-Convert-athenabillingmetrics"
    },
    "m365-Collect-athenabillingmetrics-BusinessFailed": {
      "Type": "Fail",
      "Error": "BusinessFailed",
      "Cause": "Collect Lambda returned status=failed"
    },
    "m365-Collect-athenabillingmetrics-SystemError": {
      "Type": "Fail",
      "Error": "StatesError",
      "Cause": "Collect Lambda failed with an exception/timeout"
    },
    "m365-Convert-athenabillingmetrics": {
      "Type": "Task",
      "Resource": "arn:aws:states:::lambda:invoke",
      "Parameters": {
        "FunctionName": "arn:aws:lambda:ap-northeast-1:XXXXXXXXXX:function:m365AthenaBillingMetricsConvVpc:$LATEST",
        "Payload.$": "$"
      },
      "Catch": [
        {
          "ErrorEquals": [
            "States.ALL"
          ],
          "ResultPath": "$.ConvertError",
          "Next": "m365-Convert-athenabillingmetrics-SystemError"
        }
      ],
      "Retry": [
        {
          "ErrorEquals": [
            "States.ALL"
          ],
          "BackoffRate": 1,
          "JitterStrategy": "FULL",
          "IntervalSeconds": 5,
          "MaxAttempts": 10
        }
      ],
      "TimeoutSeconds": 900,
      "OutputPath": "$.Payload",
      "Next": "m365-Convert-athenabillingmetrics-CheckStatus"
    },
    "m365-Convert-athenabillingmetrics-CheckStatus": {
      "Type": "Choice",
      "Choices": [
        {
          "And": [
            {
              "Variable": "$",
              "IsPresent": true
            },
            {
              "Variable": "$",
              "IsString": true
            },
            {
              "Variable": "$",
              "StringMatches": "*\"status\"*\"failed\"*"
            }
          ],
          "Next": "m365-Convert-athenabillingmetrics-BusinessFailed"
        }
      ],
      "Default": "m365-Convert-athenabillingmetrics-Success"
    },
    "m365-Convert-athenabillingmetrics-BusinessFailed": {
      "Type": "Fail",
      "Error": "BusinessFailed",
      "Cause": "Convert Lambda returned status=failed"
    },
    "m365-Convert-athenabillingmetrics-SystemError": {
      "Type": "Fail",
      "Error": "StatesError",
      "Cause": "Convert Lambda failed with an exception/timeout"
    },
    "m365-Convert-athenabillingmetrics-Success": {
      "Type": "Succeed"
    }
  }
}